{
  "name": "fc-web-creator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "npx eslint . --ext .js,.ts,.tsx,.jsx && npx tsc",
    "lint-fix": "npx eslint --fix . --ext .js,.ts",
    "prepare": "husky install",
    "client:deploy": "serverless client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm",
    "client:build:deploy": "yarn build && yarn client:deploy",
    "cloudfront:setup": "sls deploy",
    "cloudfront:invalidateCache": "sls invalidateCloudFrontCache",
    "cloudfront:build:deploy": "yarn client:build:deploy && yarn cloudfront:invalidateCache",
    "cloudfront:update:build:deploy": "yarn cloudfront:setup && yarn cloudfront:build:deploy"
  },
  "dependencies": {
    "@craco/craco": "6.4.5",
    "@fortawesome/fontawesome-svg-core": "6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "6.2.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@tanstack/react-table": "^8.7.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "classnames": "2.3.2",
    "formik": "2.2.9",
    "hoist-non-react-statics": "^3.3.2",
    "lodash": "4.17.21",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-dropdown": "^1.11.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "serverless-finch": "4.0.0",
    "serverless-single-page-app-plugin": "./plugins/serverless-single-page-app-plugin",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/node": "^16.11.56",
    "@types/react-color": "^3.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "prettier": "2.7.1",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
